<h2>Manage Photos</h2>

<p>
  <table style="border:none;" >
    <tr>
    </tr>
  
    <% for photo in @photos %>
    <tr>
      <td>
        <a href="<%= photo.photo.url(:medium) %>" rel="highslide" class="highslide" >
          <%= image_tag photo.photo.url(:thumb) %>
        </a>
      </td>
      <td>
        <table>
          <tr colspan="3">
            <%= photo.photo_file_name %>
            <br />
            <h3><%= photo.page_title %></h3>
          </tr>
          <tr>
            <td><%= link_to 'Show', photo %></td>
            <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
            <td><%= link_to 'Destroy', photo, :confirm => 'Are you sure?', :method => :delete %></td>
          </tr>
        </table>
      </td>
    </tr>
    <% end %>
  </table>
</p>
<br />
<p>
  <%= link_to 'New Photo', new_photo_path %>
</p>
<br />