<div id="loading" style="float: left;"></div>
<div id="content">
  <h2>Upcoming Events</h2>
  <p>
    <% for event in @events %>
      <p>
        <span class="style1"><%=h event.title %></span><br />
        <b><%=h event.start_date.strftime("%m/%d") %> - <%=h event.end_date.strftime("%m/%d") %></b><br />
        <%=h event.start_time.strftime("%I:%M%p")%> - <%=h event.end_time.strftime("%I:%M%p")%><br /> 
        <%= event.body %>
      </p>
      <br />
    <% end %>
  </p>
</div>